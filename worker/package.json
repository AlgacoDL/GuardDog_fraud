{
  "name": "guarddog-fraud-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker for GuardDog AI fraud advisory system",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --local --config wrangler.dev.toml",
    "mock": "node mock-server.js",
    "test:perf": "node test-performance.js",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "build": "npx wrangler deploy --dry-run --outdir=dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "shopify",
    "fraud",
    "webhook",
    "hmac",
    "idempotency"
  ],
  "author": "GuardDog AI",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "wrangler": "^4.28.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
